# Application Configuration
VITE_APP_NAME=IIT ML Service
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_URL=http://localhost:8000
VITE_API_TIMEOUT=30000

# Authentication (if using external auth providers)
# VITE_CLERK_PUBLISHABLE_KEY=your_clerk_key_here
# VITE_SUPABASE_URL=your_supabase_url_here
# VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Sentry Error Tracking
# Get your DSN from https://sentry.io/
VITE_SENTRY_DSN=your_sentry_dsn_here

# Analytics (PostHog / Plausible)
# VITE_POSTHOG_KEY=your_posthog_key_here
# VITE_POSTHOG_HOST=https://app.posthog.com
# VITE_PLAUSIBLE_DOMAIN=your_domain_here

# File Upload (Upload Thing / Cloudinary)
# VITE_UPLOADTHING_SECRET=your_uploadthing_secret_here
# VITE_CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name_here

# Stripe (if payments are needed)
# VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_TRACKING=false
VITE_ENABLE_FILE_UPLOAD=false

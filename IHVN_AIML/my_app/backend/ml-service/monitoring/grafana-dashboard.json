{
  "dashboard": {
    "id": null,
    "title": "IIT ML Service - Production Monitoring",
    "tags": ["iit", "ml", "production", "monitoring"],
    "timezone": "UTC",
    "panels": [
      {
        "id": 1,
        "title": "Service Health Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"iit-ml-service\"}",
            "legendFormat": "Service Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        }
      },
      {
        "id": 2,
        "title": "API Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(iit_api_errors_total[5m]) * 100",
            "legendFormat": "Error Rate (%)"
          }
        ],
        "yAxes": [
          {
            "unit": "percent",
            "min": 0
          }
        ]
      },
      {
        "id": 3,
        "title": "Prediction Latency (P95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(iit_prediction_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Latency (s)"
          }
        ],
        "yAxes": [
          {
            "unit": "seconds",
            "min": 0
          }
        ]
      },
      {
        "id": 4,
        "title": "System Resources",
        "type": "graph",
        "targets": [
          {
            "expr": "iit_system_cpu_usage_percent",
            "legendFormat": "CPU Usage (%)"
          },
          {
            "expr": "(iit_system_memory_usage_bytes / iit_system_memory_total_bytes) * 100",
            "legendFormat": "Memory Usage (%)"
          }
        ],
        "yAxes": [
          {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        ]
      },
      {
        "id": 5,
        "title": "Database Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(iit_db_query_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Query Latency (s)"
          },
          {
            "expr": "iit_db_connection_pool_checked_out",
            "legendFormat": "Active Connections"
          }
        ]
      },
      {
        "id": 6,
        "title": "Model Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "iit_model_auc_score",
            "legendFormat": "AUC Score"
          },
          {
            "expr": "iit_model_drift_detected",
            "legendFormat": "Drift Detected"
          }
        ]
      },
      {
        "id": 7,
        "title": "Cache Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(iit_api_cache_hits_total[5m]) / (rate(iit_api_cache_hits_total[5m]) + rate(iit_api_cache_misses_total[5m])) * 100",
            "legendFormat": "API Cache Hit Rate (%)"
          },
          {
            "expr": "rate(iit_feature_store_cache_hits_total[5m]) / (rate(iit_feature_store_cache_hits_total[5m]) + rate(iit_feature_store_cache_misses_total[5m])) * 100",
            "legendFormat": "Feature Store Cache Hit Rate (%)"
          }
        ],
        "yAxes": [
          {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        ]
      },
      {
        "id": 8,
        "title": "Active Alerts",
        "type": "table",
        "targets": [
          {
            "expr": "ALERTS{alertstate=\"firing\", service=\"iit-ml-service\"}",
            "legendFormat": "{{ alertname }}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "color-text"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Error Distribution by Category",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(rate(iit_error_events_total[1h])) by (category)",
            "legendFormat": "{{ category }}"
          }
        ]
      },
      {
        "id": 10,
        "title": "Incident Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(iit_incident_response_time_seconds_bucket[1h]))",
            "legendFormat": "P95 Response Time (s)"
          }
        ],
        "yAxes": [
          {
            "unit": "seconds",
            "min": 0
          }
        ]
      },
      {
        "id": 11,
        "title": "Prediction Throughput",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(iit_model_predictions_total[5m])",
            "legendFormat": "Predictions/min"
          }
        ],
        "yAxes": [
          {
            "unit": "reqps",
            "min": 0
          }
        ]
      },
      {
        "id": 12,
        "title": "Risk Distribution",
        "type": "barchart",
        "targets": [
          {
            "expr": "rate(iit_prediction_risk_distribution[5m])",
            "legendFormat": "{{ risk_level }}"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}
